{
  "host": "localhost",
  "port": 3030,
  "baseUrl": "/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "proxy" : {
    "target": "/api"
  },
  "ninject" : {
      "async" : false
  },
  "io" : {
    "port" : 3029,
    "pingTimeout" : 5000,
    "pingInterval" : 600000,
    "maxHttpBufferSize" : 1048576,
    "transports" : ["polling", "websocket"],
    "cookie" : "io",
    "cookiePath" : "/",
    "cookieHttpOnly" : true
  },
  "authentication" : {
    "secret" : "tomorrow_auth",
    "entity" : "user",
    "service": "/api/authentication",
    "authStrategies" : ["jwt"],
    "jwtOptions" : {
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    }
  },
  "mysql" : {
      "client" : "mysql2", 
      "connection" : {
          "host" : "localhost",
          "user" : "root",
          "password" : "123456",
          "database" : "tomorrow"
      },
      "pool" : {
        "min" : 0,
        "max" : 20
      },
      "useNullAsDefault" : false
  },
  "ts": true
}
