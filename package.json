{
  "name": "innocence",
  "version": "1.0.0",
  "description": "一款两个人聊天App",
  "main": "index.ts",
  "scripts": {
    "dev:node": "cross-env NODE_ENV=development ts-node-dev --no-notify src/",
    "build:node": "cross-env NODE_ENV=production npm run compile && node lib/",
    "webpack:node": "cross-env NODE_ENV=development webpack -p -w --config ./webpack/webpack.node.js --progress --hide-modules",
    "webpack:server": "cross-env NODE_ENV=development webpack -p -w --config ./webpack/webpack.service.js --progress --hide-modules",
    "webpack:client": "cross-env NODE_ENV=development webpack -p -w --config ./webpack/webpack.client.js --progress --hide-modules",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "keywords": [
    "vue",
    "websocket",
    "nodejs",
    "webchat",
    "chat"
  ],
  "author": "SongQian",
  "license": "ISC",
  "dependencies": {
    "@feathersjs/configuration": "^4.4.3",
    "@feathersjs/express": "^4.4.3",
    "@feathersjs/feathers": "^4.4.3",
    "@feathersjs/socketio": "^4.4.3",
    "@types/webpack-env": "^1.15.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "element-ui": "^2.13.0",
    "helmet": "^3.21.2",
    "inversify": "^5.0.1",
    "knex": "^0.20.4",
    "mysql2": "^2.0.2",
    "objection": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.11",
    "vue-tsx-support": "^2.3.3",
    "vuex": "^3.1.2",
    "vuex-router-sync": "^5.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.45",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.1",
    "shx": "^0.3.2",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.7.3",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
